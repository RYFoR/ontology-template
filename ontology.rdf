<?xml version="1.0"?>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:ex="http://example.org/ontology#">

  <!-- Classes -->
  <rdf:Description rdf:about="http://example.org/ontology#Person">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Person</rdfs:label>
    <rdfs:comment>A class representing a person.</rdfs:comment>
  </rdf:Description>

  <!-- Properties -->
  <rdf:Description rdf:about="http://example.org/ontology#hasName">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>hasName</rdfs:label>
    <rdfs:comment>Relates a person to their name.</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.org/ontology#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>

</rdf:RDF>
